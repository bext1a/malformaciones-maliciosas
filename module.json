{
    "id": "mmorton",
    "title": "Manual de Morton sobre Malformaciones Maliciosas",
    "description": "Lesiones permanentes, con diferente importancia, duraciones y consecuencias, según los 13 tipos de daños existentes. En Español!",
    "version": "0.1.2",
    "authors": [
        {
            "name": "bextia",
            "email": "jatomillo@gmail.com",
            "url": "https://github.com/bext1a"
        }
    ],
    "languages": [
        {
            "lang": "en",
            "name": "English",
            "path": "languages/en.json"
        },        
        {
            "lang": "es",
            "name": "Spanish",
            "path": "languages/es.json"
        }
    ],
    "scripts": [
        "scripts/main.js",
        "scripts/config.js",
        "scripts/module.js"
    ],
    "styles": [
        "styles/module.css"
    ],
    "packs": [
        {
            "name": "mmmm",
            "label": "Manual de Morton - Tablas",
            "path": "packs/tables.db",
            "type": "RollTable",
            "private": false
        },
        {
            "name": "mmmmi",
            "label": "Manual de Morton - Accesorios",
            "path": "packs/items.db",
            "type": "Item",
            "private": false,
            "system": "dnd5e"
        },
        {
            "name": "mmmmj",
            "label": "Manual de Morton sobre Malformaciones Maliciosas",
            "path": "packs/journals.db",
            "type": "JournalEntry",
            "private": false
        }
    ],
    "url": "https://github.com/bext1a/mmorton",
    "manifest": "https://github.com/bext1a/mmorton/releases/download/mmorton-v0.1.2/module.json",
    "download": "https://github.com/bext1a/mmorton/releases/download/mmorton-v0.1.2/module.zip",
    "socket": true,
    "relationships": {
        "requires": [
            {
                "id": "socketlib",
                "type": "module",
                "compatibility": {}
            }
        ]
    },
    "compatibility": {
        "minimum": "10",
        "verified": "10"
    },
    
    "minimumCoreVersion": "10"
}
